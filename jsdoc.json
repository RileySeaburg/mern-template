{
    "source": {
      "include": ["./server", "./readme.md"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "readme": "./README.md"
    }
  }